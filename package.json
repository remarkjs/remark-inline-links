{
  "name": "remark-inline-links",
  "version": "1.0.0",
  "description": "Transform references and definitions into normal links and images",
  "license": "MIT",
  "keywords": [
    "reference",
    "definition",
    "to",
    "normal",
    "link",
    "image",
    "remark"
  ],
  "dependencies": {
    "unist-util-visit": "^1.0.0"
  },
  "repository": "wooorm/remark-inline-links",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^12.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-slug": "^3.0.0",
    "remark-usage": "^2.0.0",
    "remark-validate-links": "^2.0.0",
    "tape": "^4.0.0"
  },
  "scripts": {
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "bundle": "browserify index.js --no-builtins -s remarkInlineLinks > remark-inline-links.js",
    "postbundle": "esmangle remark-inline-links.js > remark-inline-links.min.js",
    "build-md": "remark . --quiet",
    "build": "npm run bundle && npm run build-md"
  }
}
